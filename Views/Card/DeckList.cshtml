@{
}

@model List<string>

<form action="../Card/CardList" method="get">
    <input type="text" name="cardName" placeholder="Search for card" />
    <input type="submit" />
</form>

@for (int i = 0; i < Model.Count; i++)
{ 

    @if (Model[i].StartsWith("https:"))
    { 
        <img style="width:17.5%; height:17.5%" src="@Model[i]" />
    }
}   


